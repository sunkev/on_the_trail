<%= simple_form_for(@notification, html: { class: 'form-horizontal' }) do |f| %>
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :phone %>
    <%= f.input :email %>
    <%= f.input :contact_email %>
    <%= f.input :message %>
    <%= f.input :latitude, input_html: {value: session[:geo_location]['lat'].round(3)}  %>
    <%= f.input :longitude, input_html: {value: session[:geo_location]['lng'].round(3)}  %>
    <%= f.button :submit %>
<% end %>
